{
  "name": "Feedly Filter",
  "version": "1.0.7",
  "manifest_version": 2,
  "description": "Filter articles in Feedly by popularity.",
  "permissions": [
    "http://feedly.com/",
    "http://*.feedly.com/",
    "https://feedly.com/",
    "https://*.feedly.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://feedly.com/*",
        "http://*.feedly.com/*",
        "https://feedly.com/*",
        "https://*.feedly.com/*"
      ],
      "css": ["filter.css"],
      "js": [
        "util.js",
        "throttle.js",
        "eventtargetimpl.js",
        "model.js",
        "feedly.js",
        "filter.js",
        "main.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
